<div id="server-settings">
  <h2 id="tc-display-name-header"><i class='fa fa-server'></i><span id="tc-display-name-header-text">{{displayName}}</span></h2>
  <form action="#" id="update-server-form" name="add-server-form">
    <div class="form-row">
      <input type="text" name="tc-display-name" class="tc-display-name" placeholder="Display Name" value="{{displayName}}" />
    </div>
    <div class="form-row">
      <input type="url" name="tc-url" class="tc-url" placeholder="TeamCity URL" value="{{url}}" />
    </div>
    <div class="form-row">
      <input type="text" name="tc-user" class="tc-user" placeholder="Username" value="{{username}}" />
    </div>
    <div class="form-row">
      <input type="password" name="tc-pass" class="tc-pass" placeholder="Password" value="{{password}}" />
    </div>
    <input type="submit" name="update-server-submit" id="update-server-button" value="Update Server" />
  </form>
  <h3>Projects</h3>
  <div id="projects-and-builds-list-container"></div>
</div>